{
    "authorizedGrantTypes": [
        "password",
        "refresh_token",
        "authorization_code",
        "implicit"
    ],
    "additionalInformation": {},
    "clientId": "jhipsterapp",
    "scope": [
        "read",
        "write"
    ],
    "accessTokenValiditySeconds": 1800,
    "clientSecret": "my-secret-token-to-change-in-production",
    "_class": "<%=packageName%>.domain.OAuth2AuthenticationClientDetails",
    "refreshTokenValiditySeconds": 2000,
    "authorities": [
        {
            "role": "ROLE_ADMIN",
            "_class": "org.springframework.security.core.authority.SimpleGrantedAuthority"
        },
        {
            "role": "ROLE_USER",
            "_class": "org.springframework.security.core.authority.SimpleGrantedAuthority"
        }
    ],
    "resourceIds": [
        "res_jhipster"
    ]
}
